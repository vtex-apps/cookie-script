{
  "name": "cookie-script",
  "vendor": "vtex",
  "version": "0.1.1",
  "title": "Cookie Script",
  "description": "Make your website cookies comply with GDPR and CCPA.",
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "builders": {
    "pixel": "0.x",
    "docs": "0.x"
  },
  "dependencies": {},
  "settingsSchema": {
    "title": "Cookie Script",
    "type": "object",
    "properties": {
      "cookieScriptId": {
        "title": "ID",
        "type": "string"
      },
      "geoTargeting": {
        "title": "Geo-targeting",
        "type": "string",
        "default": "eu",
        "enum": [
          "chs03",
          "eu",
          "ca",
          "ca-eu"
        ],
        "enumNames": [
          "All visitors",
          "EU",
          "California",
          "EU + California"
        ]
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
